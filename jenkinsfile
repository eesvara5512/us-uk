pipeline {			
    agent any
	 parameters {
		//string(name: 'environment', defaultValue: 'dev', description: 'Environment to build for (Valid values: Dev, Test, Prod)')
	          choice(choices: 'dev','test','prod' description: 'What is env', name: 'environment')					
                    }

    stages{
        stage('Build'){	
			
            steps{			
			sh 'cd "${environment}"'
                        sh  'pwd'
                        sh 'sh /var/lib/jenkins/workspace/paramters-test/dev/dev.sh'
			}
        }			
    }			
}
